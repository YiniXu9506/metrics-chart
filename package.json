{
  "name": "metrics-chart",
  "version": "1.0.11",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "distDir": "dist/index.js",
      "optimize": true,
      "includeNodeModules": {
        "@elastic/charts": false,
        "ahooks": false,
        "react": false,
        "react-dom": false
      }
    },
    "theme": {
      "source": "src/theme/index.tsx",
      "distDir": "dist/theme",
      "optimize": true,
      "includeNodeModules": false,
      "outputFormat": "commonjs"
    },
    "scss2css": {
      "source": [
        "src/light_theme.scss",
        "src/dark_theme.scss"
      ],
      "distDir": ".css"
    },
    "css_theme": {
      "source": [
        ".css/light_theme.css",
        ".css/dark_theme.css"
      ],
      "distDir": "dist/theme"
    }
  },
  "dependencies": {
    "@baurine/grafana-value-formats": "^1.0.5",
    "dayjs": "^1.11.5",
    "react-use": "^17.4.0",
    "string-template": "^1.0.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/react": "^17.0.2",
    "autoprefixer": "^10.4.19",
    "parcel": "^2.7.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-modules": "^6.0.0",
    "postcss-prefixwrap": "^1.47.0",
    "sass": "^1.75.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@elastic/charts": "^47.2.1",
    "ahooks": "^3.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "npm run build:css && parcel start",
    "build": "npm run build:css && npm run build:css-theme && npm run build:theme && parcel build --target main --no-optimize && npm run build:type",
    "build:css": "parcel build --target scss2css",
    "build:css-theme": "parcel build --target css_theme",
    "build:theme": "parcel build --target theme --no-optimize",
    "build:type": "npx -p typescript tsc --outDir dist/types",
    "build:analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
