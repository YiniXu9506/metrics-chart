{
  "name": "metrics-chart",
  "version": "0.19.0",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "distDir": "dist/index.js",
      "optimize": true,
      "includeNodeModules": {
        "@elastic/charts": false,
        "ahooks": false,
        "axios": false,
        "react": false,
        "react-dom": false
      }
    }
  },
  "dependencies": {
    "@baurine/grafana-value-formats": "^1.0.4",
    "dayjs": "^1.11.5",
    "react-use": "^17.4.0",
    "string-template": "^1.0.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/react": "^17.0.2",
    "parcel": "^2.7.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@elastic/charts": "^46.10.1",
    "ahooks": "^3.7.0",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "parcel start",
    "build": "parcel build --no-optimize && npm run build:type",
    "build:type": "npx -p typescript tsc --outDir dist/types",
    "build:analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
